public async validateOperatorOptions_cf1(operatorTypeValidation: string) {
    const page = this.page;

    // Get the dropdown (select element)
    const selectLocator = page.locator('select'); // Update selector if needed
    await selectLocator.waitFor({ state: 'attached', timeout: 15000 });

    // Log all option texts
    const options = selectLocator.locator('option');
    const optionCount = await options.count();
    console.log(`Total options found: ${optionCount}`);

    for (let i = 0; i < optionCount; i++) {
        const optionText = await options.nth(i).textContent();
        const optionHtml = await options.nth(i).evaluate(el => el.outerHTML);
        console.log(`Option ${i}: ${optionText?.trim()} => ${optionHtml}`);
    }
}

