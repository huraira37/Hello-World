public async searchCustomerExtranet(phoneNumber: string) {
  const page = this.page;

  this.logger.log("Phone number is " + phoneNumber);

  // Fill phone number and click search
  await this.phoneNumber.fill(phoneNumber);
  this.logger.log("Entered the phone number");

  await this.runSearchBtn.click();
  this.logger.log("Clicked on search button");

  // Wait for page load
  await page.waitForLoadState('domcontentloaded');

  // Wait until title is updated
  await expect(page).toHaveTitle("Customer Service Workbench", { timeout: 10000 });

  // Wait until the heading appears
  await expect(this.cswPageHeading).toContainText("Customer Service Workbench", { timeout: 10000 });

  this.logger.log("Customer Service page is appeared");
}


await Promise.all([
  page.waitForLoadState('domcontentloaded'),
  this.runSearchBtn.click()
]);
